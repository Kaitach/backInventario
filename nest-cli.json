{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/command/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/command/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/command",
  "projects": {
    "command": {
      "type": "application",
      "root": "apps/command",
      "entryFile": "main",
      "sourceRoot": "apps/command/src",
      "compilerOptions": {
        "tsConfigPath": "apps/command/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "queri-comander": {
      "type": "application",
      "root": "apps/queri-comander",
      "entryFile": "main",
      "sourceRoot": "apps/queri-comander/src",
      "compilerOptions": {
        "tsConfigPath": "apps/queri-comander/tsconfig.app.json"
      }
    },
    "query": {
      "type": "application",
      "root": "apps/query",
      "entryFile": "main",
      "sourceRoot": "apps/query/src",
      "compilerOptions": {
        "tsConfigPath": "apps/query/tsconfig.app.json"
      }
    },
    "proxy": {
      "type": "application",
      "root": "apps/proxy",
      "entryFile": "main",
      "sourceRoot": "apps/proxy/src",
      "compilerOptions": {
        "tsConfigPath": "apps/proxy/tsconfig.app.json"
      }
    },
    "jwt-auth": {
      "type": "application",
      "root": "apps/jwt-auth",
      "entryFile": "main",
      "sourceRoot": "apps/jwt-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/jwt-auth/tsconfig.app.json"
      }
    }
  }
}